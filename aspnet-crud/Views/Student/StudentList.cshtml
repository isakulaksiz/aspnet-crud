@model List<aspnet_crud.Context.tbl_Student>

@{
    ViewBag.Title = "StudentList";
}

<table class="table table-hover">
    <tr>
        <th>Name</th>
        <th>Fname</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Description</th>
        <th>Action</th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Name</td>
        <td>@item.Fname</td>
        <td>@item.Email</td>
        <td>@item.Mobile</td>
        <td>@item.Description</td>
        <td>
            <a href="#"><i class="glyphicon glyphicon-pencil"></i></a>
            <a href="#"><i class="glyphicon glyphicon-trash"></i></a>
        </td>
    </tr>
    }
</table>

